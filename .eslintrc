{
    "env": {
        "browser": true,
        "es6": true
    },
    "parser": "vue-eslint-parser",
    "extends": "eslint:recommended",
    "parserOptions": {
        "parser": "babel-eslint",
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "warn",
            "single"
        ],
        "semi": [
            "warn",
            "always"
        ],
        "no-console": 0
    }
}